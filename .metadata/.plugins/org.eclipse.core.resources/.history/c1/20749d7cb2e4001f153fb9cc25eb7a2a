package metodes;
import java.util.*;
public class asdasdasdasdas {
	ArrayList<String> lista = new ArrayList<>();
	lista.add("Hola");
	lista.add("Mundo");
	System.out.println(lista);    // [Hola, Mundo]

	lista.get(0);  // "Hola"
	lista.set(1, "Java");  // [Hola, Java]
	lista.remove(0);  // [Java]

}
